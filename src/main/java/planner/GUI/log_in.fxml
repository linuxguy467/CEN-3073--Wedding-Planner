<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import java.net.URL?>
<AnchorPane prefWidth="600.0" styleClass="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="planner.Controller.logInScene">

    <stylesheets>
        <URL value="@Login.css"/>
    </stylesheets>
    <HBox prefHeight="10.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <MenuBar prefHeight="25.0" prefWidth="601.0" HBox.hgrow="ALWAYS">
            <Menu mnemonicParsing="false" text="File">
                <MenuItem fx:id="closeButton" mnemonicParsing="false" text="Close"/>
            </Menu>
            <Menu mnemonicParsing="false" text="Settings">
                <MenuItem mnemonicParsing="false" text="Configure"/>
                <MenuItem fx:id="createUserButton" mnemonicParsing="false" text="Create User"/>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
                <MenuItem mnemonicParsing="false" text="About"/>
            </Menu>
        </MenuBar>
    </HBox>
    <Text id="logInText" layoutX="176.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please Log In!"/>
    <HBox layoutX="175.0" layoutY="145.0" prefHeight="77.0" prefWidth="251.0" AnchorPane.bottomAnchor="178.0"
          AnchorPane.leftAnchor="175.0" AnchorPane.rightAnchor="174.0" AnchorPane.topAnchor="145.0">
        <VBox prefHeight="210.0" prefWidth="362.0" spacing="25.0">
            <TextField fx:id="userNameTextField" prefHeight="25.0" prefWidth="250.0" promptText="username"/>
            <PasswordField fx:id="passwordField" prefWidth="250.0" promptText="password"/>
        </VBox>
    </HBox>
    <Button fx:id="logInButton" layoutX="355.0" layoutY="249.0" mnemonicParsing="false" prefHeight="25.0"
            prefWidth="73.0" text="Log In!"/>
    <Label fx:id="wrongLogInText" layoutX="272.0" layoutY="320.0" prefHeight="17.0" prefWidth="82.0"
           textFill="#f51111"/>
</AnchorPane>
